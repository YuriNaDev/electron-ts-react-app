{
	"compilerOptions": {
		"target": "ESNext",
		"lib": ["DOM", "DOM.Iterable", "ESNext"],
		"module": "CommonJS",
		"allowJs": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"noImplicitAny": true,
		"sourceMap": true,
		"strict": true,
		"moduleResolution": "node",
		"resolveJsonModule": true,
		// "noEmit": true,
		"jsx": "react",
		"baseUrl": ".",
		"outDir": "dist",
		"paths": {
			"*": ["node_modules/*"]
		}
	},
	"include": ["src/**/*"]
}
